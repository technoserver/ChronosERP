    <subDataset name="DatasetMovimento" uuid="b86a869f-40e0-4170-ba99-09baf455d021">
        <property name="com.jaspersoft.studio.data.sql.tables" value=""/>
        <property name="com.jaspersoft.studio.data.defaultdataadapter" value="DataAdapter.xml"/>
        <parameter name="idmovimento" class="java.lang.Integer"/>
        <queryString>
            <![CDATA[SELECT
    codigo,
    descricao,
    Sum(total) as total
FROM
    (
    SELECT
	 t.codigo,
	 t.descricao || (CASE
            WHEN ((f.forma = '04' or f.forma = '03') and f.bandeira = '01') THEN ' (Visa) '
            WHEN ((f.forma = '04' or f.forma = '03') and f.bandeira = '02') THEN ' (Mastercard) '
            WHEN ((f.forma = '04' or f.forma = '03') and f.bandeira = '03') THEN ' (American Express) '
            WHEN ((f.forma = '04' or f.forma = '03') and f.bandeira = '04') THEN ' (Sorocred) '
            WHEN ((f.forma = '04' or f.forma = '03') and f.bandeira = '06') THEN ' (Elo) '
            WHEN ((f.forma = '04' or f.forma = '03') and f.bandeira = '07') THEN ' (Hipercard) '
            WHEN ((f.forma = '04' or f.forma = '03') and f.bandeira = '08') THEN ' (Aura) '
            WHEN ((f.forma = '04' or f.forma = '03') and f.bandeira = '09') THEN ' (Cabal) '
            ELSE ''
            END) as descricao,
		(f.valor-f.troco) as total
	FROM pdv_movimento m
	INNER JOIN pdv_venda_cabecalho v ON v.id_pdv_movimento = m.id
	INNER JOIN pdv_forma_pagamento f ON f.id_pdv_venda_cabecalho = v.id
	INNER JOIN tipo_pagamento t ON f.id_tipo_pagamento = t.id
	WHERE m.id =   $P{idmovimento} and v.status_venda in('F','E')

	UNION ALL

	SELECT
		'01',
		'Dinheiro',
		s.valor * -1
	FROM pdv_movimento m
	INNER JOIN pdv_sangria s ON s.id_pdv_movimento = m.id
	WHERE m.id =   $P{idmovimento}

	UNION ALL

	SELECT
		'01',
		'Dinheiro',
		s.valor
	FROM pdv_movimento m
	INNER JOIN pdv_suprimento s ON s.id_pdv_movimento = m.id
	WHERE m.id = $P{idmovimento}

    UNION ALL

SELECT
        t.tipo,
        t.descricao,
        r.valor_recebido
FROM pdv_movimento m
INNER JOIN fin_parcela_recebimento r ON r.id_pdv_movimento = m.id
INNER JOIN fin_tipo_recebimento t ON r.id_fin_tipo_recebimento = t.id
WHERE m.id = $P{idmovimento}

    UNION ALL
		SELECT
			t.codigo,
			t.descricao || (CASE
            	WHEN ((f.forma = '04' or f.forma = '03') and f.bandeira = '01') THEN ' (Visa) '
            	WHEN ((f.forma = '04' or f.forma = '03') and f.bandeira = '02') THEN ' (Mastercard) '
            	WHEN ((f.forma = '04' or f.forma = '03') and f.bandeira = '03') THEN ' (American Express) '
            	WHEN ((f.forma = '04' or f.forma = '03') and f.bandeira = '04') THEN ' (Sorocred) '
            	WHEN ((f.forma = '04' or f.forma = '03') and f.bandeira = '06') THEN ' (Elo) '
            	WHEN ((f.forma = '04' or f.forma = '03') and f.bandeira = '07') THEN ' (Hipercard) '
            	WHEN ((f.forma = '04' or f.forma = '03') and f.bandeira = '08') THEN ' (Aura) '
            	WHEN ((f.forma = '04' or f.forma = '03') and f.bandeira = '09') THEN ' (Cabal) '
            	ELSE ''
            END) as descricao,
			(f.valor-f.troco) as total
		FROM pdv_movimento m
		JOIN os_abertura o ON o.id_movimento = m.id
		JOIN os_forma_pagamento f ON f.id_os_abertura = o.id
		JOIN tipo_pagamento t ON f.id_tipo_pagamento = t.id
		WHERE m.id =   $P{idmovimento} and o.status in(12,13) )  pagamentos
GROUP BY codigo, descricao;]]>
        </queryString>
        <field name="codigo" class="java.lang.String"/>
        <field name="descricao" class="java.lang.String"/>
        <field name="total" class="java.math.BigDecimal"/>
        <group name="codigo">
            <groupExpression><![CDATA[$F{codigo}]]></groupExpression>
        </group>
    </subDataset>
    <parameter name="idmovimento" class="java.lang.Integer"/>
    <queryString>
        <![CDATA[SELECT
            concat(m.id , s.id , 1) AS id,
            o.login as operador,
            m.id AS idmovimento,
            s.id AS idorigem,
            s.data_sangria AS data_hora,
            COALESCE(s.observacao,'Saída - Sangria') AS descricao,
            s.valor,
            '01' AS codigo_forma_pagamento,
            'Dinheiro' AS forma_pagamento,
            'S' AS tipo,
            'Sangria' AS origem,
            s.observacao,
            empresa.razao_social AS empresa_razao_social,
            empresa.nome_fantasia AS empresa_nome_fantasia,
            empresa.cnpj AS empresa_cnpj,
            empresa.inscricao_estadual AS empresa_inscricao_estadual
            FROM pdv_movimento m
            JOIN pdv_sangria s ON s.id_pdv_movimento = m.id
            JOIN pdv_operador o ON o.id = m.id_pdv_operador
            INNER JOIN empresa empresa ON m.id_empresa = empresa.id
            INNER JOIN empresa_endereco empresa_endereco ON empresa.id = empresa_endereco.id_empresa
            WHERE m.id =  $P{idmovimento}

            UNION
            SELECT
            concat(m.id , s.id , 2) AS id,
            o.login as operador,
            m.id AS idmovimento,
            s.id AS idorigem,
            s.data_suprimento AS data_hora,
            COALESCE(s.observacao,'Entrada - Acrécimo') AS descricao,
            s.valor,
            '01' AS codigo_forma_pagamento,
            'Dinheiro' AS forma_pagamento,
            'E' AS tipo,
            'Suprimentos' AS origem,
            s.observacao,
            empresa.razao_social AS empresa_razao_social,
            empresa.nome_fantasia AS empresa_nome_fantasia,
            empresa.cnpj AS empresa_cnpj,
            empresa.inscricao_estadual AS empresa_inscricao_estadual
            FROM
            pdv_movimento m
            JOIN pdv_suprimento s ON s.id_pdv_movimento = m.id
            JOIN pdv_operador o ON o.id = m.id_pdv_operador
            INNER JOIN empresa empresa ON m.id_empresa = empresa.id
            INNER JOIN empresa_endereco empresa_endereco ON empresa.id = empresa_endereco.id_empresa
            WHERE m.id  = $P{idmovimento}

            UNION
            SELECT
            concat(m.id , f.id , 3) AS id,
            o.login as operador,
            m.id AS idmovimento,
            v.id AS idorigem,
            v.data_hora_venda AS data_hora,
            ('Venda n°' || v.id) || ' - ' || v.nome_cliente || ' (PDV)' AS descricao,
            f.valor-f.troco,
            t.codigo AS codigo_forma_pagamento,
            t.descricao AS forma_pagamento,
            'E' AS tipo,
            'VENDA' AS origem,
            '' AS observacao,
            empresa.razao_social AS empresa_razao_social,
            empresa.nome_fantasia AS empresa_nome_fantasia,
            empresa.cnpj AS empresa_cnpj,
            empresa.inscricao_estadual AS empresa_inscricao_estadual
            FROM pdv_movimento m
            INNER JOIN pdv_venda_cabecalho v ON v.id_pdv_movimento = m.id
            INNER JOIN pdv_forma_pagamento f ON f.id_pdv_venda_cabecalho = v.id
            INNER JOIN tipo_pagamento t ON f.id_tipo_pagamento = t.id
            INNER JOIN pdv_operador o ON o.id = m.id_pdv_operador
            INNER JOIN empresa empresa ON m.id_empresa = empresa.id
            INNER JOIN empresa_endereco empresa_endereco ON empresa.id = empresa_endereco.id_empresa
            WHERE m.id =  $P{idmovimento}

            and (v.status_venda = 'E' or v.status_venda = 'F')
UNION
            SELECT
            concat(m.id , f.id , 3) AS id,
            '',
            m.id AS idmovimento,
            o.id AS idorigem,
            o.data_fim AS data_hora,
            ('OS n°' || o.id) || '(OS)' AS descricao,
            f.valor-f.troco,
            t.codigo AS codigo_forma_pagamento,
            t.descricao AS forma_pagamento,
            'E' AS tipo,
            'OS' AS origem,
            '' AS observacao,
            empresa.razao_social AS empresa_razao_social,
            empresa.nome_fantasia AS empresa_nome_fantasia,
            empresa.cnpj AS empresa_cnpj,
            empresa.inscricao_estadual AS empresa_inscricao_estadual
            FROM pdv_movimento m
            INNER JOIN os_abertura o ON o.id_movimento = m.id
            INNER JOIN os_forma_pagamento f ON f.id_os_abertura = o.id
            INNER JOIN tipo_pagamento t ON f.id_tipo_pagamento = t.id
            INNER JOIN empresa empresa ON m.id_empresa = empresa.id
            INNER JOIN empresa_endereco empresa_endereco ON empresa.id = empresa_endereco.id_empresa
            WHERE m.id =  $P{idmovimento}
            and (o.status = 13 or o.status = 12)
	  union
            select
                concat(m.id , r.id , 4) AS id,
                o.login as operador,
                m.id AS idmovimento,
                r.id AS idorigem,
                r.data_recebimento AS data_hora,
                ('Recebimento n°' || r.id) || ' de '|| ps.nome  AS descricao,
                r.valor_recebido,
                '01' AS codigo_forma_pagamento,
                'Dinheiro' AS forma_pagamento,
                'E' AS tipo,
                'RECEBIMENTO' AS origem,
                '' AS observacao,
	  empresa.razao_social AS empresa_razao_social,
            empresa.nome_fantasia AS empresa_nome_fantasia,
            empresa.cnpj AS empresa_cnpj,
            empresa.inscricao_estadual AS empresa_inscricao_estadual
            from pdv_movimento m
            inner join pdv_operador o ON o.id = m.id_pdv_operador
            inner join fin_parcela_recebimento r on r.id_pdv_movimento = m.id
            inner join fin_parcela_receber p on p.id = r.id_fin_parcela_receber
            inner join fin_lancamento_receber l on l.id = p.id_fin_lancamento_receber
            inner join cliente c on c.id = l.id_cliente
            inner join pessoa ps on ps.id = c.id_pessoa
	  INNER JOIN empresa empresa ON m.id_empresa = empresa.id
            INNER JOIN empresa_endereco empresa_endereco ON empresa.id = empresa_endereco.id_empresa
	  WHERE m.id =  $P{idmovimento}
            ORDER BY 11,8,5;]]>
    </queryString>
