    <queryString>
        <![CDATA[SELECT
     v.id,
     p.nome as cliente,
     pv.nome as vendedor,
     v.situacao,
     v.valor_subtotal,
     v.valor_desconto,
     v.valor_total,
     v.valor_comissao,
     v.data_venda,
     n.numero,
     empresa.razao_social AS empresa_razao_social,
     empresa.nome_fantasia AS empresa_nome_fantasia,
     empresa.cnpj AS empresa_cnpj,
     empresa.inscricao_estadual AS empresa_inscricao_estadual,
     empresa.imagem_logotipo AS empresa_imagem_logotipo,
     empresa_endereco.logradouro AS empresa_endereco_logradouro,
     empresa_endereco.numero AS empresa_endereco_numero,
     empresa_endereco.complemento AS empresa_endereco_complemento,
     empresa_endereco.bairro AS empresa_endereco_bairro,
     empresa_endereco.cidade AS empresa_endereco_cidade,
     empresa_endereco.cep AS empresa_endereco_cep,
     empresa_endereco.fone AS empresa_endereco_fone,
     empresa_endereco.uf AS empresa_endereco_uf,
     empresa.email AS empresa_email
FROM
     venda_cabecalho v
     INNER JOIN cliente c ON v.id_cliente = c.id
     INNER JOIN pessoa p ON c.id_pessoa = p.id
     INNER JOIN vendedor vd ON vd.id = v.id_vendedor
     INNER JOIN colaborador cl ON vd.id_colaborador = cl.id
     INNER JOIN pessoa pv ON cl.id_pessoa = pv.id
     INNER JOIN empresa empresa ON v.id_empresa = empresa.id
     INNER JOIN empresa_endereco empresa_endereco ON empresa.id = empresa_endereco.id_empresa
     LEFT JOIN nfe_cabecalho n ON n.id_venda_cabecalho = v.id
WHERE empresa.id = $P{idempresa} and  empresa_endereco.principal = 'S' and (v.id_cliente =  $P{idcliente} or $P{idcliente} is null )
and (v.id_vendedor =  $P{idvendedor} or $P{idvendedor} is null) and (v.situacao = $P{situacao} or $P{situacao} is null)
 and v.data_venda BETWEEN $P{dataPedidoInicial} and $P{dataPedidoFinal} and (v.id =$P{id} or $P{id} is null)
union
SELECT
     os.id,
     p.nome as cliente,
     pv.nome as vendedor,
     cast(os.status as text) as status,
     (os.valor_total_servicos + os.valor_total_produtos - os.valor_total_desconto ) as valor_subtotal,
     os.valor_total_desconto,
     (os.valor_total_servicos + os.valor_total_produtos) as valor_total,
     os.valor_comissao,
     os.data_fim as data_venda,
     n.numero,
     empresa.razao_social AS empresa_razao_social,
     empresa.nome_fantasia AS empresa_nome_fantasia,
     empresa.cnpj AS empresa_cnpj,
     empresa.inscricao_estadual AS empresa_inscricao_estadual,
     empresa.imagem_logotipo AS empresa_imagem_logotipo,
     empresa_endereco.logradouro AS empresa_endereco_logradouro,
     empresa_endereco.numero AS empresa_endereco_numero,
     empresa_endereco.complemento AS empresa_endereco_complemento,
     empresa_endereco.bairro AS empresa_endereco_bairro,
     empresa_endereco.cidade AS empresa_endereco_cidade,
     empresa_endereco.cep AS empresa_endereco_cep,
     empresa_endereco.fone AS empresa_endereco_fone,
     empresa_endereco.uf AS empresa_endereco_uf,
     empresa.email AS empresa_email
FROM
     os_abertura os
     INNER JOIN cliente c ON os.id_cliente = c.id
     INNER JOIN pessoa p ON c.id_pessoa = p.id
     LEFT JOIN vendedor vd ON vd.id = os.id_vendedor
     INNER JOIN colaborador cl ON vd.id_colaborador = cl.id
     INNER JOIN pessoa pv ON cl.id_pessoa = pv.id
     INNER JOIN empresa empresa ON os.id_empresa = empresa.id
     INNER JOIN empresa_endereco empresa_endereco ON empresa.id = empresa_endereco.id_empresa
     LEFT JOIN nfe_cabecalho n ON n.id_venda_cabecalho = os.id
WHERE empresa.id = $P{idempresa} and  empresa_endereco.principal = 'S'
and (os.id_cliente =  $P{idcliente} or $P{idcliente} is null )
and (os.id_vendedor =  $P{idvendedor} or $P{idvendedor} is null)
and (os.status = $P{status_os} or $P{status_os} is null)
and os.data_fim BETWEEN $P{dataPedidoInicial} and $P{dataPedidoFinal} and (os.id =$P{id} or $P{id} is null)
union
SELECT
     v.id,
     COALESCE(v.nome_cliente, 'Cliente não identificado') AS cliente,
     pv.nome as vendedor,
     v.status_venda,
     v.valor_subtotal,
     v.valor_desconto,
     v.valor_total,
     v.valor_comissao,
     v.data_hora_venda as data_venda,
     n.numero,
     empresa.razao_social AS empresa_razao_social,
     empresa.nome_fantasia AS empresa_nome_fantasia,
     empresa.cnpj AS empresa_cnpj,
     empresa.inscricao_estadual AS empresa_inscricao_estadual,
     empresa.imagem_logotipo AS empresa_imagem_logotipo,
     empresa_endereco.logradouro AS empresa_endereco_logradouro,
     empresa_endereco.numero AS empresa_endereco_numero,
     empresa_endereco.complemento AS empresa_endereco_complemento,
     empresa_endereco.bairro AS empresa_endereco_bairro,
     empresa_endereco.cidade AS empresa_endereco_cidade,
     empresa_endereco.cep AS empresa_endereco_cep,
     empresa_endereco.fone AS empresa_endereco_fone,
     empresa_endereco.uf AS empresa_endereco_uf,
     empresa.email AS empresa_email
FROM pdv_venda_cabecalho v
INNER JOIN vendedor vd ON vd.id = v.id_vendedor
INNER JOIN colaborador cl ON vd.id_colaborador = cl.id
INNER JOIN pessoa pv ON cl.id_pessoa = pv.id
LEFT JOIN nfe_cabecalho n ON n.id = v.id_nfe_cabecalho
INNER JOIN empresa empresa ON v.id_empresa = empresa.id
INNER JOIN empresa_endereco empresa_endereco ON empresa.id = empresa_endereco.id_empresa
WHERE empresa.id = $P{idempresa}  and  empresa_endereco.principal = 'S'
and (v.id_cliente  = $P{idcliente}  or $P{idcliente} is null )
and (v.id_vendedor =  $P{idvendedor} or $P{idvendedor} is null)
and (v.status_venda = $P{situacao} or $P{situacao} is null)
and v.data_hora_venda BETWEEN $P{dataPedidoInicial} and $P{dataPedidoFinal}
and (v.id =$P{id} or $P{id} is null)
order by $P!{ordem}]]>
    </queryString>
